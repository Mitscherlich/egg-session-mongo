{
  "name": "egg-session-mongo",
  "version": "2.0.2",
  "description": "MongoDB store for egg session. based on `connect-mongo`.",
  "main": "index.js",
  "eggPlugin": {
    "name": "sessionMongo"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "session",
    "mongo",
    "MongoDB"
  ],
  "dependencies": {
    "koa2-session-mongo": "^0.1.0",
    "mongodb": "^5.0.0"
  },
  "devDependencies": {
    "autod": "3.1.2",
    "autod-egg": "1.1.0",
    "egg": "3.17.4",
    "egg-bin": "6.4.1",
    "egg-ci": "2.2.0",
    "egg-mock": "5.10.8",
    "eslint": "8.46.0",
    "eslint-config-egg": "12.2.1",
    "mongoose": "7.4.1",
    "mz-modules": "2.1.0",
    "supertest": "6.3.3",
    "webstorm-disable-index": "1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
